        <element id="hw_paesaggistica" title="Nascondi se paesaggistica" type="PlominoHidewhen">
          <id type="Products.Archetypes.Field.StringField">hw_paesaggistica</id>
          <Formula type="Products.Archetypes.Field.TextField"><![CDATA[if context.isDocument():
    app = context.getItem('iol_tipo_app','')
else:
    app = context.getForm().getFormName().split('_')[1]
    
if app == 'paesaggistica':
    return True
else:
    return False]]></Formula>
          <isDynamicHidewhen type="Products.Archetypes.Field.BooleanField">False</isDynamicHidewhen>
        </element>
