      <beforeSaveDocument type="Products.Archetypes.Field.TextField"><![CDATA[]]></beforeSaveDocument>
      <onSaveDocument type="Products.Archetypes.Field.TextField"><![CDATA[context.setItem('Form','frm_presentata')
#### se il datagrid è popolato aggiorno i valori dei campi
if context.getItem('oneri_calcolati_dg'):
    rows=context.getItem('oneri_calcolati_dg',[])
    sup=0;cc=0;b1=0;b2=0
    for row in rows:
        sup=sup+row[9]
        cc=cc+row[10]
        b1=b1+row[11]
        b2=b2+row[12]
        
    context.setItem('oneri_sup',float(sup))
    context.setItem('oneri_cc',float(cc))
    context.setItem('oneri_b1',float(b1))
    context.setItem('oneri_b2',float(b2))]]></onSaveDocument>
